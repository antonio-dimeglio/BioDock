{
  "id": "fastqc-base",
  "name": "FastQC",
  "description": "The base pipeline for fastQC.",
  "command": ["fastqc", "--outdir=/data/output", "/data/input/*.fastq"],
  "version": "1.0.0",
  "inputFileTypes": ["fastq", "fq", "fastq.gz", "fq.gz"],
  "outputFileTypes": ["html", "zip"],
  "inputDirectory": "/data/input",
  "outputDirectory": "/data/output"
}